import os
import numpy as np
import matplotlib.pyplot as plt
def rSVD(X, r, q=1, p=0):
ny=X.shape[1]
P=np.random.randint(ny, size=r+p)
Z=X[:, P]
for k in range(q):
Z=X.dot(X.T.dot(Z))
Q, R=np.linalg.qr(Z, mode="reduced")
Y=Q.T.dot(X)
UY, S, V=np.linalg.svd(Y, full_matrices=False)
U=Q.dot(UY)
return U, S, V
