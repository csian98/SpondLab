setwd("/Users/csian/Desktop/CP/Conovlution/data")
before<-read.table("data.txt")
data_pos<-"/Users/csian/Desktop/CP/Conovlution/Source/truncated/k/"
graph_pos<-"/Users/csian/Desktop/CP/Conovlution/Source/truncated/graph2/"
setwd(data_pos)
file_name="k_"
tmp=paste(data_pos, file_name, 16, ".txt", sep="")
after<-read.table(tmp)
size<-length(before[, 1])
# normalization
before[, 2]<-scale(before[, 2], center=min(before[, 2]), scale=max(before[, 2])-min(before[, 2]))
for(i in 1:(size-1)){
tmp=paste(data_pos, file_name, i, ".txt", sep="")
png_name=paste(graph_pos, file_name, i, ".png", sep="")
after<-read.table(tmp)
after[, 1]<-scale(after[, 1], center=min(after[, 1]), scale=max(after[, 1])-min(after[, 1]))
diff<-which.max(before[, 2])-which.max(after[, 1])
tmp<-paste(file_name, i, sep="")
png(png_name, width=800, height=600, res=300)
plot(before[(diff+1):size, 1], before[(diff+1):size, 2], type='l', col="blue", xlab="Kinetic Energy[eV]", ylab="CPS", main=tmp)
lines(before[(diff+1):size, 1], after[1:(size-diff), 1])
tmp<-paste("Shift ", diff, " ticks", sep="")
text(1, 0.8, tmp)
dev.off()
}
