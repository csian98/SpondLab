}
normalize<-function(data){
return(data/sum(data))
}
raw_data<-read_data(paste(path, file_name, '_0_reg1.spc', sep=''))
raw_data[, 2]<-normalize(raw_data[, 2])
for(i in conta){
tmp_data=read_data(paste(path, file_name, '_', i, '_reg1.spc', sep=''))
tmp_data[, 2]<-normalize(tmp_data[, 2])
diff_max<-max(tmp_data[, 2]-raw_data[, 2])
#sprintf("conta_%d max_value : %f", i, diff_max)
cat(i, " : ", diff_max, "\n")
}
